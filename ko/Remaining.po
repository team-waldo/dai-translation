msgid "German"
msgstr "독일어"

#. FILE = Systems/Localization/LanguageDefinition_En
#. PATH = LocalizedLanguage/DisplayName
msgctxt "0x000200b6"
msgid "English"
msgstr "영어"

#. FILE = Systems/Localization/LanguageDefinition_Es
#. PATH = LocalizedLanguage/DisplayName
msgctxt "0x000200b3"
msgid "Spanish"
msgstr "스페인어"

#. FILE = Systems/Localization/LanguageDefinition_Fr
#. PATH = LocalizedLanguage/DisplayName
msgctxt "0x000200b4"
msgid "French"
msgstr "프랑스어"

#. FILE = Systems/Localization/LanguageDefinition_It
#. PATH = LocalizedLanguage/DisplayName
msgctxt "0x000200b8"
msgid "Italian"
msgstr "이탈리아어"

#. FILE = Systems/Localization/LanguageDefinition_Ja
#. PATH = LocalizedLanguage/DisplayName
msgctxt "0x000200b2"
msgid "Japanese"
msgstr "일본어"

