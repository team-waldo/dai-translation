msgid "Silver Ring"
msgstr "은반지"

#. FILE: da3_dlc_blue\designcontent\Items\dlcblue_silverring.xml
#. PATH: StackedItemAsset/Description
#: da3_dlc_blue\designcontent\Items\dlcblue_silverring.xml:11
msgctxt "0x0004b2a1"
msgid "A silver ring found in a fountain."
msgstr "분수에서 찾은 은반지."

#. FILE: da3_dlc_blue\designcontent\Items\dlcblue_stubloot_item.xml
#. PATH: StackedItemAsset/DisplayName
#: da3_dlc_blue\designcontent\Items\dlcblue_stubloot_item.xml:6
