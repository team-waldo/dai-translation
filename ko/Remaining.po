msgid "Alistair"
msgstr "알리스터"

#. FILE = da3/Actors/Party/Specializations/Follower/gen_partymember_plot_warden_stroud
#. PATH = CreatureEntityData/CharacterNameReference
msgctxt "0x00018696"
msgid "Stroud"
msgstr "스트라우드"

