msgstr "하콘 추종자 궁수"

#. FILE = DA3_DLC1/Creatures/Faction/Biotic/Human/creature_dlc1_hakkon_bruiser
#. PATH = CreatureEntityData/CharacterNameReference
msgctxt "0x000466fa"
msgid "Hakkonite Bruiser"
msgstr "하콘 추종자 거병"

#. FILE = DA3_DLC1/Creatures/Faction/Biotic/Human/creature_dlc1_hakkon_footsoldier_f
#. PATH = CreatureEntityData/CharacterNameReference
msgctxt "0x000466f7"
msgid "Hakkonite Foot Soldier"
msgstr "하콘 추종자 보병"

#. FILE = DA3_DLC1/Creatures/Faction/Biotic/Human/creature_dlc1_hakkon_guardsman
#. PATH = CreatureEntityData/CharacterNameReference
msgctxt "0x000466f2"
msgid "Hakkonite Guardsman"
msgstr "하콘 추종자 위병"

#. FILE = DA3_DLC1/Creatures/Faction/Biotic/Human/creature_dlc1_hakkon_prowler
#. PATH = CreatureEntityData/CharacterNameReference
msgctxt "0x000466f6"
msgid "Hakkonite Prowler"
msgstr "하콘 추종자 배회자"

#. FILE = DA3_DLC1/Creatures/Faction/Biotic/Human/creature_dlc1_hakkon_spellbinder
#. PATH = CreatureEntityData/CharacterNameReference
msgctxt "0x000466f8"
msgid "Hakkonite Spellbinder"
msgstr "하콘 추종자 주문결속사"

#. FILE = DA3_DLC1/Creatures/Faction/Demonic/Demon/creature_dlc1_demon_wraith_hakkon
#. PATH = CreatureEntityData/CharacterNameReference
msgctxt "0x00046d92"
msgid "Hakkon Wraith"
msgstr "하콘 사령"

#. FILE = DA3_DLC1/Creatures/Faction/Demonic/Undead/creature_dlc1_boss_hakkon
#. PATH = CreatureEntityData/CharacterNameReference
msgctxt "0x000466fb"
