msgctxt "0x000200b0"
msgid "Polish"
msgstr "폴란드어"

#. FILE = Systems/Localization/LanguageDefinition_Ru
#. PATH = LocalizedLanguage/DisplayName
msgctxt "0x000200b1"
msgid "Russian"
msgstr "러시아어"

