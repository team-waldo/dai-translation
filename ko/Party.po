msgctxt "0x00042904"
msgid ""
"<font color=\"#BAAB7B\">Damage Bonus: </font><font color=\"#F5ECCE\""
">{CUSTOM0}% for every creature within {CUSTOM1} meters</font>\n"
msgstr ""
