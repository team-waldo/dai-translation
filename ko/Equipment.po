msgstr ""

#. FILE: DA3_DLC1\Equipment\Items\Weapons\Rogue\Bows\dlc1_equip_dlc1_bow_1.xml
#. PATH: EquipItemAsset/Description
msgctxt "0x00046e17"
msgid "The fabled elven smith Tanaleth of Halamshiral once made bows for the Fade Hunters, those Emerald Knights tasked with protecting the Dalish kingdom from maleficarum and demons."
msgstr ""

#. FILE: DA3_DLC1\Equipment\Items\Weapons\Rogue\Bows\dlc1_equip_dlc1_bow_2.xml
#. PATH: EquipItemAsset/DisplayName
